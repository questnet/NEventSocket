<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>NEventSocket.DotNetCore</PackageId>
    <Authors>iamkinetic</Authors>
    <Company>iamkinetic</Company>
    <Product>NEventSocket.DotNetCore</Product>
    <Description>.Net Core port of NEventSocket</Description>
    <Version>2.0.1</Version>
    <RepositoryUrl>https://www.myget.org/F/questnet/api/v2/package</RepositoryUrl>
    <PackageTags>FreeSwitch NEventSocket DotNetCore</PackageTags>
    <PackageReleaseNotes>Initial questnet version.</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/questnet/NEventSocket</PackageProjectUrl>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Microsoft.Extensions.Logging" version="3.1.2" />
        <dependency id="System.Reactive" version="4.3.2" />
      </group>
    </dependencies>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.1.2" />
    <PackageReference Include="System.Reactive" Version="4.3.2" />
  </ItemGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>NEventSocket.Tests</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="FreeSwitch\ApiResponse.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="FreeSwitch\BackgroundJobResult.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="FreeSwitch\BasicMessage.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="FreeSwitch\ChannelState.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="FreeSwitch\CommandReply.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="FreeSwitch\ContentTypes.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="FreeSwitch\EventMessage.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="FreeSwitch\EventName.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="FreeSwitch\EventSocketLogLevel.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="FreeSwitch\FreeSwitchLogLevel.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>



</Project>
